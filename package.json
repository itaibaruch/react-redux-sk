{
  "name": "react-redux-sk",
  "version": "1.0.0",
  "description": "React + Redux + Webpack + Transform = very quick development :)",
  "repository": {
    "type": "git",
    "url": "https://github.com/itaibaruch/react-redux-sk.git"
  },
  "scripts": {
    "clean": "rimraf dist",
    "build:p": "cross-env NODE_ENV=production webpack --config webpack.config.prod.js",
    "build": "npm run clean && npm run build:p",
    "start": "node devServer.js",
    "start:p": "node server.js",
    "start:nm": "nodemon devServer.js",
    "lint": "eslint src",
    "test": "cross-env NODE_ENV=test mocha --compilers js:babel-core/register --require ./test/test_helper.js --recursive ./test",
    "test:watch": "npm run test -- --watch"
  },
  "author": "Itai Baruch",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.6.5",
    "babel-eslint": "^5.0.0-beta4",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-react-constant-elements": "^6.5.0",
    "babel-plugin-transform-react-inline-elements": "^6.6.5",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-react-hmre": "^1.1.1",
    "chai": "^3.5.0",
    "chai-jquery": "^2.0.0",
    "cross-env": "^1.0.7",
    "css-loader": "^0.23.1",
    "eslint": "^1.10.3",
    "eslint-plugin-babel": "^3.0.0",
    "eslint-plugin-react": "^3.11.3",
    "eventsource-polyfill": "^0.9.6",
    "extract-text-webpack-plugin": "^1.0.1",
    "jquery": "^2.2.1",
    "jsdom": "^8.1.0",
    "mocha": "^2.4.5",
    "nodemon": "^1.9.1",
    "react-addons-test-utils": "^0.14.7",
    "rimraf": "^2.4.3",
    "style-loader": "^0.13.1",
    "webpack": "^1.12.9",
    "webpack-dev-middleware": "^1.4.0",
    "webpack-hot-middleware": "^2.9.1"
  },
  "dependencies": {
    "axios": "^0.9.1",
    "babel-preset-stage-1": "^6.3.13",
    "body-parser": "^1.15.1",
    "express": "^4.13.4",
    "lodash": "^3.10.1",
    "react": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-redux": "^4.0.0",
    "react-router": "^2.2.1",
    "redux": "^3.0.4",
    "redux-form": "^4.1.3",
    "redux-promise": "^0.5.3"
  }
}
